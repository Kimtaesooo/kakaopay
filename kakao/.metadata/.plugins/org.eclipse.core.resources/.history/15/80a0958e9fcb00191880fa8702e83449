import com.kakaopay.model.TranHistoryVO;

public interface DataOutputMapper {
	
	// 메서드명은 마이바티스 매퍼 호출에서 사용 될 이름
	AccountInfoVO	getAcntInfo();		// 계좌정보
	MngtInfoVO		getMngtInfo();		// 관리점 정보
	TranHistoryVO	getTranHistory();	// 거래내역
	
