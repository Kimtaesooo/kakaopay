package com.kakaopay.model;

import java.util.Date;

public class AcntDataInfoModel {
	private Date	year;		// 연도
	private String	name;		// 계좌명
	private String	acct_no;	// 계좌번호
	private String	sum_amt;	// 합계(금액 - 수수료)
	
	public Date getYear() {
		return year;
	}
	public void setYear(Date year) {
		this.year = year;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getAcct_no() {
		return acct_no;
	}
	public void setAcct_no(String acct_no) {
		this.acct_no = acct_no;
	}
	public String getSum_amt() {
		return sum_amt;
	}
	public void setSum_amt(String sum_amt) {
		this.sum_amt = sum_amt;
	}
	
}
