package com.kakaopay;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

@SpringBootApplication
public class KakaopayApplication {
	
	/*
	 * 스프링 MVC 구조로 DAO(데이터 처리를 담당)에서 Mybatis를 호출하고,
	 * 여기서 처리된 데이터를 VO(데이터를 전달하는 역할) 객체에 담아 Service, Controller을 거쳐 화면으로 보낸다.
	 */

	public static void main(String[] args) {
		SpringApplication.run(KakaopayApplication.class, args);
	}

}
